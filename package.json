{
  "name": "feedget",
  "version": "1.0.9",
  "description": "FeedGet: Feedback Widget component",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "feedget",
    "feedback",
    "widget"
  ],
  "repository": {
    "url": "https://github.com/Marceometry/feedget.git",
    "type": "git"
  },
  "license": "ISC",
  "author": "Marcelino Teixeira",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.css src/**/*.svg dist/",
    "build:css": "npx tailwindcss -i src/styles/global.css -o dist/styles/global.css --minify",
    "build": "npm run clean && npx ttsc --p ./tsconfig.json && npm run copy-files && npm run build:css"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "html2canvas": "^1.4.1",
    "phosphor-react": "^1.4.1",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
